{
  "type": "object",
  "properties": {
    "personalinfo": {
      "type": "action",
      "selectContains": "Personal Information",
      "control": "property.innerHTML",
      "selector": "div.node.nodeon"
    },
    "ispersonalinfo": {
      "type": "identifier",
      "control": "exists",
      "selector": "span[id='homePathLink']"
    }
  },
  "connect": [
    {
      "match": [
        "ispersonalinfo"
      ],
      "event": "personalinfo"
    }
  ]
}
