{
  "type": "object",
  "properties": {
    "personalinfo": {
      "type": "action",
      "control": "property.innerText",
      "selector": "span[class='c']",
      "selectContains": "Personal Information",
      "selectHidden": true
    },
    "ispersonalinfo": {
      "type": "identifier",
      "control": "exists",
      "selector": "span[id='homePathLink']"
    }
  },
  "connect": [
    {
      "match": [
        "ispersonal"
      ],
      "event": "personalinfo"
    }
  ]
}
